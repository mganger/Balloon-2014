loop()
{
	getTime();		//Using Arduino Libraries, we may be able to get time from GPSrs
	getData(str,int); 	//string: ie temp, int = pin
	getPic(int,int); 	//pins for USB
	writeData(stuff); 	//stuff for where SD card is
	borp();			//end send data over radio
	counter()		/increment counter and add to data structure
}


